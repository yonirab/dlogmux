{
  "name": "dlogmux",
  "version": "1.0.0",
  "description": "Docker Containers Logs Multiplexer",
  "repository" : {
    "type": "git",
    "url": "git@bitbucket.org:yoni_rabinovitch/dlogmux.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "debuglocal": "node --inspect-brk index.js",
    "inspectlocal": "node --inspect index.js",
    "debug": "node --inspect-brk=$(hostname -i):$DEBUG_PORT index.js",
    "inspect": "node --inspect$(hostname -i):$DEBUG_PORT index.js"
  },
  "author": "Yoni Rabinovitch",
  "license": "ISC",
  "dependencies": {
    "node-docker-api": "^1.1.22"
  }
}
